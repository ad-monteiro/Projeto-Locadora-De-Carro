CREATE TABLE USUARIO (
COD_USUARIO INTEGER NOT NULL,
LOGIN VARCHAR (10) NOT NULL,
SENHA VARCHAR (100) NOT NULL,
DP_USUARIO (20) NOT NULL,
CONSTRAINT PK_USUARIO PRIMARY KEY (COD_USUARIO));


CREATE TABLE CARRO (
COD_CARRO INTEGER NOT NULL,
VALOR_DIARIA_CARRO NUMERIC (9,2) NOT NULL,
PLACA_CARRO VARCHAR (10) NOT NULL,
RENAVAM_CARRO VARCHAR (15) NOT NULL,
CONSTRAINT PK_CARRO PRIMARY KEY (COD_CARRO));


CREATE TABLE MUNICIPIO (
COD_MUNICIPIO INTEGER NOT NULL,
UF_MUNICIPIO VARCHAR (2) NOT NULL,
NOME_MUNICIPIO VARCHAR (40) NOT NULL,
CONSTRAINT PK_MUNICIPIO PRIMARY KEY (COD_MUNICIPIO));


CREATE TABLE FUNCIONARIO (
COD_FUNC INTEGER NOT NULL,
COD_USUARIO VARCHAR (10) NOT NULL,
NOME_FUNC VARCHAR (100) NOT NULL,
COD_MUNICIPIO INTEGER NOT NULL,
CEP_FUNC VARCHAR (10) NOT NULL,
UF_FUNC VARCHAR (2) NOT NULL, 
BAIRRO_FUNC VARCHAR (70) NOT NULL,
RUA_FUNC VARCHAR (70) NOT NULL,
TEL_FUNC VARCHAR (15) NOT NULL,
CONSTRAINT PK_FUNCIONARIO PRIMARY KEY (COD_FUNC),
CONSTRAINT FK_USUARIO FOREIGN KEY (COD_USUARIO) REFERENCES USUARIO,
CONSTRAINT FK_MUNICIPIO FOREIGN KEY (COD_MUNICIPIO) REFERENCES MUNICIPIO);


CREATE TABLE ALUGUEL (
COD_ALUGUEL INTEGER NOT NULL,
VALOR_ALUGUEL NUMERIC (9,2) NOT NULL, 
DATA_INIC_ALUGUEL DATE NOT NULL,
DATA_FIM_ALUGUEL DATE NOT NULL,
COD_FUNC INTEGER NOT NULL,
COD_CARRO INTEGER NOT NULL,
COD_CLIENTE INTEGER NOT NULL,
COD_MUNICIPIO INTEGER NOT NULL,
CONSTRAINT PK_ALGUEL PRIMARY KEY (COD_ALUGUEL),
CONSTRAINT FK_FUNCIONARIO FOREIGN KEY (COD_FUNC) REFERENCES FUNCIONARIO,
CONSTRAINT FK_CARRO FOREIGN KEY (COD_CARRO) REFERENCES CARRO,
CONSTRAINT FK_CLIENTE FOREIGN KEY (COD_FUNC) REFERENCES CLIENTE,
CONSTRAINT FK_MUNICIPIO FOREIGN KEY (COD_MUNICIPIO) REFERENCES MUNICIPIO);


CREATE TABLE CLIENTE (
COD_CLIENTE INTEGER NOT NULL,
NOME_CLIENTE VARCHAR (100) NOT NULL,
TEL_CLIENTE VARCHAR (15) NOT NULL,
CPF_CLIENTE VARCHAR (11) NOT NULL,
CNPJ_CLIENTE VARCHAR (20) NOT NULL,
COD_MUNICIPIO INTEGER NOT NULL,
CEP_CLIENTE VARCHAR (10) NOT NULL,
UF_CLIENTE VARCHAR (2) NOT NULL,
RUA_CLIENTE VARCHAR (70) NOT NULL,
BAIRRO_CLIENTE VARCHAR (70) NOT NULL,
CONSTRAINT PK_CLIENTE PRIMARY KEY (COD_CLIENTE),
CONSTRAINT FK_MUNICIPIO FOREIGN KEY (COD_MUNICIPIO) REFERENCES MUNICIPIO);


COMMIT WORK;
